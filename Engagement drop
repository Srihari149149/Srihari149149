import pandas as pd

# Load your file
df = pd.read_excel('your_file.xlsx')  # adjust path if needed

# Rename relevant columns
df.rename(columns={
    'netprice': 'Sales Value',
    'dealer code with name': 'Dealer Code',
    'material code': 'Product Code',
    'Date': 'Invoice Date'
}, inplace=True)

# Convert Invoice Date and extract Month
df['Invoice Date'] = pd.to_datetime(df['Invoice Date'], dayfirst=True, errors='coerce')
df['Month'] = df['Invoice Date'].dt.to_period('M')
